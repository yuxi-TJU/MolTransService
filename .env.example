# LLM provider can be "openai" or "gemini"
LLM_PROVIDER=openai

# Default model name for the provider
LLM_MODEL=gpt5-nano

# API credentials (fill with your real key/URL)
LLM_API_KEY=your-api-key-here
LLM_API_URL=https://api.openai.com/v1/chat/completions

# Optional default reports directory
REPORTS_DIR=./mol_trans_service/report_generator/report_database
OUTPUT_DIR=./generated_outputs
